    <connection>Profile App</connection>
    <sql>SELECT
  profile_source_table_value_distribution_summary_analysis_pk
, profile_source_name
, profile_source_table_name
, profile_source_table_column_name
, pstvdsa.dw_revision
, DATE_FORMAT(pstvdsa.w_create_dt, &apos;%Y%m%d&apos;) AS create_date
, DATE_FORMAT(pstvdsa.w_create_dt, &apos;%H:%i&apos;) AS create_time
, pstvdsa.w_update_dt
, pstvdsa.w_end_dt
, null_count
, unique_count
, distinct_count
FROM profile_source_table_value_distribution_summary_analysis pstvdsa
JOIN profile_source_table_column pstc ON pstc.profile_source_table_column_pk = pstvdsa.profile_source_table_column_pk
JOIN profile_source_table pst ON pstc.profile_source_table_pk = pst.profile_source_table_pk
JOIN profile_source ps ON pst.profile_source_fk = ps.profile_source_id
WHERE pstvdsa.w_update_dt &gt;= ?
</sql>
    <limit>0</limit>
    <lookup>Table input 2</lookup>
    <execute_each_row>N</execute_each_row>
    <variables_active>Y</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
