delimiter $$

CREATE DATABASE `profile` /*!40100 DEFAULT CHARACTER SET latin1 */$$

USE profile$$

CREATE TABLE `profile_source_connection` (
  `profile_source_connection_id` int(11) NOT NULL AUTO_INCREMENT,
  `profile_source_connection_name` varchar(75) NOT NULL,
  `profile_source_tunnel_script` varchar(500) DEFAULT NULL,
  `w_create_dt` datetime DEFAULT NULL,
  `w_update_dt` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `w_end_dt` datetime NOT NULL DEFAULT '2037-12-31 23:59:59',
  PRIMARY KEY (`profile_source_connection_id`)
) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=latin1$$

CREATE TABLE `profile_source` (
  `profile_source_id` int(11) NOT NULL AUTO_INCREMENT,
  `profile_source_name` varchar(75) NOT NULL,
  `profile_source_connection` int(11) NOT NULL,
  `w_create_dt` datetime NOT NULL,
  `w_update_dt` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `w_end_dt` datetime NOT NULL DEFAULT '2037-12-31 11:59:59',
  PRIMARY KEY (`profile_source_id`),
  KEY `profile_source_fk1` (`profile_source_connection`),
  CONSTRAINT `profile_source_fk1` FOREIGN KEY (`profile_source_connection`) REFERENCES `profile_source_connection` (`profile_source_connection_id`) ON DELETE NO ACTION ON UPDATE NO ACTION
) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=latin1$$

CREATE TABLE `profile_sampling_query` (
  `profile_sampling_query_id` int(11) NOT NULL AUTO_INCREMENT,
  `profile_sampling_query` text,
  `profile_sampling_query_source` int(11) DEFAULT NULL,
  `profile_source_table_name` varchar(150) NOT NULL,
  `w_create_dt` datetime DEFAULT NULL,
  `w_update_dt` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `w_end_dt` datetime DEFAULT '2037-12-31 11:59:59',
  PRIMARY KEY (`profile_sampling_query_id`),
  KEY `profile_sampling_query_fk1` (`profile_sampling_query_source`),
  CONSTRAINT `profile_sampling_query_fk1` FOREIGN KEY (`profile_sampling_query_source`) REFERENCES `profile_source` (`profile_source_id`) ON DELETE NO ACTION ON UPDATE NO ACTION
) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=latin1$$

delimiter ;


